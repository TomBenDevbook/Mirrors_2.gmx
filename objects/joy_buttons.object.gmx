            <string>///Joystick control II - 10 (restart) 1 (enter)



//detection
joy_id=-1;
if joystick_exists(1) joy_id=1; else if joystick_exists(2) joy_id=2;


if joy_id!=-1
{

if room!=Menu { window_set_cursor(cr_none); } else { window_set_cursor(cr_default); }

if joystick_check_button(joy_id, 10) room_goto(room);
if joystick_check_button(joy_id, 1) keyboard_key_release(vk_enter);
if joystick_check_button(joy_id, 9) keyboard_key_release(vk_escape);
if joystick_check_button(joy_id, 4) { keyboard_key_press(vk_shift); last_shift=true; } else if last_shift==true { keyboard_key_release(vk_shift); last_shift=false; };

if joystick_check_button(joy_id, 5) last_pressed=5;
else if last_pressed==5 { last_pressed=0; menu_driven--; if menu_driven&lt;1 { menu_driven=15; } menu_mouse_autocenter(); }
if joystick_check_button(joy_id, 6) last_pressed=6;
else if last_pressed==6 { last_pressed=0; menu_driven++; if menu_driven&gt;15 { menu_driven=1; } menu_mouse_autocenter(); }
 

}
else
{
 window_set_cursor(cr_default);
}







</string>
