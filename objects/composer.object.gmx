if mirror_x==-1
shift_x=(sprite_get_width(sprite_index)-sprite_get_xoffset(sprite_index));
else
shift_x=0;

if mirror_y==-1
shift_y=(sprite_get_height(sprite_index)-sprite_get_yoffset(sprite_index));
else
shift_y=0;

draw_surface_ext(reflect,x+shift_x,y+shift_y,mirror_x,mirror_y,0,c_white,1); 
}

