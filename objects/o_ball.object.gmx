if joy_id!=-1
{

switch(joystick_direction(joy_id))
{
//horizontal, vertical
  case vk_numpad8: motion_add(90,1); break;
  case vk_numpad4: motion_add(180,1); break;
  case vk_numpad2: motion_add(270,1); break;
  case vk_numpad6: motion_add(0,1); break;

